<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%.12thread] %-5level %.16logger - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
        <logger name="io.shiftleft.passes.CpgPass" level="WARN" />
        <logger name="com.github.plume.oss.drivers.IDriver$" level="WARN" />
        <logger name="com.github.plume.oss.passes.reachingdef.PlumeReachingDefPass" level="WARN" />
    </Loggers>
</Configuration>